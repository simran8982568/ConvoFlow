# Production Environment Configuration
NODE_ENV=production

# API Configuration
VITE_API_BASE_URL=https://api.ayuchat.com
VITE_API_VERSION=v1

# Feature Flags
VITE_ENABLE_SUPERADMIN=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_AUTOMATION=true
VITE_ENABLE_TEMPLATES=true

# Branding Configuration
VITE_APP_NAME=AyuChat
VITE_APP_LOGO=/assets/images/logo.png
VITE_BRAND_COLOR=teal

# File Upload Configuration
VITE_MAX_FILE_SIZE=16777216
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,video/mp4,video/webm

# Development Configuration
VITE_DEV_MODE=false
VITE_MOCK_API=true
VITE_DEBUG_ENABLED=false
